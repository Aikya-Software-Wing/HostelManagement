@model List<HostelManagement.Areas.HostelMessManagement.Models.TransactionsViewModel>


<table class="table table-striped">
    <thead>
        <tr>
            <th>Tid</th>
            <th>Bid</th>
            <th>Date</th>
            <th>Payment Type</th>
            <th>Account Head</th>
            <th>Bank Name</th>
            <th>Academic Year</th>
            <th>Transaction</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>@item.id</td>
                <td>@item.bid</td>
                <td>@item.dateOfPay</td>
                <td>@item.paymentType</td>
                <td>@item.accountHead</td>
                <td>@item.bankName</td>
                <td>@item.academicYear</td>
                <td>@item.transaction</td>
                <td>@item.amount</td>
            </tr>
        }
    </tbody>
</table>
