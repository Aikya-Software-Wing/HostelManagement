@using (@Html.BeginForm("GenerateReportByDate", "User", FormMethod.Post))
{
    <div id="BidRangeDiv">
        <div class="row">
            <fieldset class="form-group">
                <div class="form-group col-xs-3">
                    <label>Start Date</label>
                    <input name="StartDate" id="dateId" class="form-control" type="date" required />
                </div>
                <div class="form-group col-xs-3">
                    <label>End Date</label>
                    <input name="EndDate" id="dateId1" class="form-control" type="date" required />
                </div>
                <div class="form-group col-xs-3">
                </div>
                <div class="form-group col-xs-3">
                </div>
            </fieldset>
        </div>
    </div>
    <div class="row">
        @Html.ValidationSummary()
    </div>
    <div class="row">
        <fieldset class="form-group">
            <div class="form-group col-xs-3">
                <input type="submit" class="btn btn-primary btn-block" />
            </div>
        </fieldset>
    </div>
}
<script type="text/javascript">
    $(document).ready(function () {
        if (!Modernizr.inputtypes.date) {
            $("#dateId1").datepicker();
            $("#dateId").datepicker();
        }

    });
</script>