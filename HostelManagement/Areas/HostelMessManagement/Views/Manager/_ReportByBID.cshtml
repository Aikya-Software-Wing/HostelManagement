@using (@Html.BeginForm("GenerateReportByBID", "Manager", FormMethod.Post))
{
    <div id="BidRangeDiv">
        <div class="row">
            <fieldset class="form-group">
                <div class="form-group col-xs-3">
                    <label>Start BID</label>
                    <input name="StartBid" class="form-control" type="text" />
                </div>
                <div class="form-group col-xs-3">
                    <label>End BID </label>
                    <input name="EndBid" class="form-control" type="text" />
                </div>
                <div class="form-group col-xs-3">
                </div>
                <div class="form-group col-xs-3">
                </div>
            </fieldset>
        </div>
    </div>
    <div class="row">
        @Html.ValidationSummary()
    </div>
    <div class="row">
        <fieldset class="form-group">
            <div class="form-group col-xs-3">
                <input type="button" value="Add Range" class="btn btn-link btn-block" id="btnAdd" />
            </div>
            <div class="form-group col-xs-3">
                <input type="submit" class="btn btn-primary btn-block" />
            </div>
        </fieldset>
    </div>
}

<script type="text/javascript">
    $("#btnAdd").click(function () {
        var div = $("<div />");
        div.html(GetDynamicDivValue(""));
        $("#BidRangeDiv").append(div);
    });
    function GetDynamicDivValue() {
        return "<div class=\"row\">"
            + "<fieldset class=\"form-group\">"
            + "<div class=\"form-group col-xs-3\">"
            + "<label>Start BID</label>"
            + "<input name=\"StartBid\" class=\"form-control\" type=\"text\" />"
            + "</div>"
            + "<div class=\"form-group col-xs-3\">"
            + "<label>End BID </label>"
            + "<input name=\"EndBid\" class=\"form-control\" type=\"text\" />"
            + "</div>"
            + "<div class=\"form-group col-xs-3\">"
            + "</div>"
            + "<div class=\"form-group col-xs-3\">"
            + "</div>"
            + "</fieldset>"
            + "</div>";
    }
</script>